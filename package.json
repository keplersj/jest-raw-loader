{
  "name": "jest-raw-loader",
  "version": "1.0.1",
  "description": "Jest transformer mimicking webpack-contrib/raw-loader's functionality",
  "main": "index.js",
  "repository": "https://github.com/keplersj/jest-raw-loader",
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "jest --bail --findRelatedTests"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-starstuff": "1.5.6",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-runner-eslint": "1.0.0",
    "jest-runner-prettier": "0.3.7",
    "lint-staged": "12.0.3",
    "prettier": "2.4.1"
  },
  "jest": {
    "collectCoverage": true,
    "projects": [
      {
        "displayName": "test",
        "collectCoverage": true
      },
      {
        "displayName": "lint:prettier",
        "preset": "jest-runner-prettier",
        "testPathIgnorePatterns": [
          "/node_modules/",
          "/coverage/"
        ]
      },
      {
        "displayName": "lint:eslint",
        "runner": "eslint",
        "testMatch": [
          "<rootDir>/**/*.js"
        ],
        "testPathIgnorePatterns": [
          "/node_modules/",
          "/coverage/"
        ]
      }
    ]
  },
  "eslintConfig": {
    "extends": "starstuff/auto",
    "env": {
      "node": true
    }
  }
}
